<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="10dp"
                android:color="@color/outline_color" />

            <solid android:color="#f3eff1" />

        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:top="-1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#f3eff1" />

            <solid android:color="#f3eff1" />
        </shape>
    </item>

</layer-list>